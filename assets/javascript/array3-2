let fruits = ['apple', 'grape'];
fruits.push('banana');
fruits.push('strawberry');

let extractFruit = function(e) {
    return function() {
        return "I picked: " + e.shift();
    };
};

let extract = extractFruit(fruits);

console.log(extract());
console.log(extract());
console.log(extract());
console.log(extract());
console.log(extract());
